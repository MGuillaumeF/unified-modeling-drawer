{
  "author": "MGuillaumeF",
  "name": "unified-modeling-drawer",
  "version": "0.1.0",
  "private": true,
  "main": "./packaging/build/electron.js",
  "scripts": {
    "build:app": "npm run copy:app:model && cd app && webpack --define-process-env-node-env production --progress --color --env PUBLIC_PATH=\"'.'\"",
    "build:packaging": "npm run copy:packaging:model && cd packaging && tsc --project tsconfig.json",
    "build:doc": "styleguidist build",
    "start": "npm run copy:app:model && cd app && webpack serve --open --progress --color --define-process-env-node-env development",
    "cy:build": "cd cypress && tsc --project tsconfig.json",
    "postcy:build": "mv cypress/cypress.config.js . || move cypress\\cypress.config.js .",
    "precy:open": "npm run cy:build",
    "preci:test": "npm run cy:build",
    "cy:open": "cypress open",
    "prettier": "prettier --write . --ignore-path ./.gitignore",
    "lint:eslint": "eslint . --fix --format html --output-file ./dist/reports/lint/eslint-report.html --ignore-path ./.gitignore",
    "lint:eslint:json": "eslint . --fix --format json --output-file ./dist/reports/lint/eslint-report.json --ignore-path ./.gitignore",
    "lint:stylelint": "stylelint \"**/*.{css,scss,sass}\" --custom-syntax postcss-scss --fix --formatter json > ./dist/reports/lint/stylelint-report.json --ignore-path ./.gitignore",
    "unit:test": "nyc mocha --config test/runners/mocha/.mocharc.jsonc --reporter mocha-multi-reporters --reporter-options configFile=mocha-multi-config.json",
    "unit:test:packaging": "nyc mocha --config packaging/test/runners/mocha/.mocharc.jsonc --reporter mocha-multi-reporters --reporter-options configFile=mocha-multi-config.json",
    "e2e:test": "cypress run --e2e",
    "ci:test": "start-server-and-test http://localhost:3000",
    "test": "npm run unit:test && npm run e2e:test",
    "webpack-stats": "webpack --define-process-env-node-env production --profile --json=stats.json",
    "precopy:app": "mkdir ./packaging/build/app || mkdir packaging\\build\\app || echo \"Directory not created\"",
    "copy:app": "cp --recursive app/build/* packaging/build/app || xcopy app\\build\\* packaging\\build\\app /Y /E",
    "precopy:app:model": "mkdir ./app/src/.model || mkdir app\\src\\.model || echo \"Directory not created\"",
    "copy:app:model": "cp --recursive model/* app/src/.model || xcopy model app\\src\\.model /Y /E",
    "precopy:packaging:model": "mkdir ./packaging/src/.model || mkdir packaging\\src\\.model || echo \"Directory not created\"",
    "copy:packaging:model": "cp --recursive model/* packaging/src/.model || xcopy model packaging\\src\\.model /Y /E",
    "copy:model": "npm run copy:app:model && copy:packaging:model",
    "precopy:resources": "mkdir ./packaging/build/resources || mkdir packaging\\build\\resources || echo \"Directory not created\"",
    "copy:resources": "cp --recursive packaging/resources/* packaging/build/resources || xcopy packaging\\resources packaging\\build\\resources /Y /E",
    "copy:statics": "npm run copy:app && npm run copy:resources",
    "preel:run": "npm run build:packaging && npm run copy:statics",
    "preel:package": "npm run preel:run",
    "el:run": "electron ./packaging/build/electron.js",
    "el:package": "electron-builder"
  },
  "build": {
    "appId": "com.umd.app",
    "productName": "UMD - Unified Modeling Drawer",
    "copyright": "Copyright Â© 2023 MGuillaumeF",
    "mac": {
      "category": "public.app.category.utilities"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": ["x64", "ia32"]
        },
        {
          "target": "msi",
          "arch": ["x64", "ia32"]
        },
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        },
        {
          "target": "zip",
          "arch": ["x64", "ia32"]
        }
      ]
    },
    "files": ["app/build/**/*", "packaging/build/**/*"],
    "directories": {
      "buildResources": "packaging/resources"
    },
    "publish": null
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all", "not IE 11"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.11.0",
    "@gfx/zopfli": "^1.0.15",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mguillaumef/audiso": "^0.0.4",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/recommended": "^1.0.2",
    "@types/chai": "^4.3.5",
    "@types/jest": "^29.5.3",
    "@types/mocha": "^10.0.1",
    "@types/mochawesome": "^6.2.1",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.6",
    "@types/sinon": "^10.0.15",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.14",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cypress": "^12.17.2",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-sonarqube-reporter": "^1.12.0",
    "dotenv": "^16.3.1",
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3",
    "electron-installer-dmg": "^4.0.0",
    "electron-packager": "^17.1.1",
    "electron-winstaller": "^5.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-react": "^0.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-perf": "^3.3.1",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "imports-loader": "^4.0.1",
    "jest": "^29.6.2",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^10.2.0",
    "mocha-multi-reporters": "^1.5.1",
    "mocha-sonarqube-reporter": "^1.0.2",
    "mochawesome": "^7.1.3",
    "node-sass": "^9.0.0",
    "nyc": "^15.1.0",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.0.0",
    "react-docgen-typescript": "^2.2.2",
    "react-styleguidist": "^13.1.1",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "sinon": "^15.2.0",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^2.0.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "stylelint-prettier": "^4.0.1",
    "stylelint-webpack-plugin": "^4.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "wait-on": "^7.0.1",
    "web-vitals": "^3.4.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-visualizer-plugin2": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "react-router-dom": "^6.14.2",
    "react-transition-group": "^4.4.5"
  },
  "dependencies": {
    "i18next": "^23.4.2",
    "jsonschema": "^1.4.1",
    "xml2js": "^0.6.2"
  }
}
